/******************************************************* home grid setup, second pass*/
.grid_aural {
  display: grid;
  grid-template-columns: 1fr 1fr 1fr 1fr;
  gap: 16px;
  margin:16px;
  overflow:hidden;
}
.grid_aural .grid__item {
  position: relative;
  width: 100%;
}
.grid_aural .grid__item:nth-child(1) {
  grid-column: 1/3;
  grid-row: 1/3;
}
.grid_aural .grid__item:nth-child(1)::before {
  padding-top: 100%;
}
.grid_aural .grid__item:nth-child(2) {
  grid-column: 3 / 4;
  justify-self: center;
}
.grid_aural .grid__item:nth-child(2)::before {
  padding-top: calc(4/5 * 100%);
}
.grid_aural .grid__item:nth-child(3) {
  grid-column: 4 / 5;
}
.grid_aural .grid__item:nth-child(3)::before {
  padding-top: calc(4/5 * 100%);
}
.grid_aural .grid__item:nth-child(4) {
  grid-column: 3 / 5;
  grid-row: 2/4;
}
.grid_aural .grid__item:nth-child(4)::before {
	padding-top: 100%;
	overflow: hidden;
	margin-bottom: -210px;
}
.grid_aural .grid__item:nth-child(5) {
  grid-row: 3/5;
  grid-column: 1 / 3;
}
.grid_aural .grid__item:nth-child(5)::before {
  padding-top: calc(4/5 * 100%);
}
.grid_aural .grid__item:nth-child(6) {
  grid-row: 4/5;
  grid-column: 3 / 4;
}
.grid_aural .grid__item:nth-child(6)::before {
  padding-top: calc(4/5 * 100%);
}
.grid_aural .grid__item:nth-child(7) {
  grid-row: 4/5;
  grid-column: 4/5;
}
.grid_aural .grid__item:nth-child(7)::before {
  padding-top: calc(4/5 * 100%);
}
.grid_aural .grid__item:nth-child(8) {
  grid-row: 5/7;
  grid-column: 1/3;
}
.grid_aural .grid__item:nth-child(8)::before {
  padding-top: calc(4/5 * 100%);
}
.grid_aural .grid__item:nth-child(9) {
  grid-row: 5/7;
  grid-column: 3 /5;
}
.grid_aural .grid__item:nth-child(9)::before {
  padding-top: calc(4/5 * 100%);
}
.grid_aural .grid__item.lifestyle.ethos {
  grid-column: 1/5;
}
.grid_aural .grid__item.lifestyle.ethos::before {
  padding-top: calc(4/10 * 100%);
}
.grid_aural .grid__item::before {
  content: "";
  height: 0;
  display: inline-block;
  padding-top: 100%;
  width: 1px;
  position: relative;
}
.grid_aural .grid__item .grid__subgrid {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  display: grid;
  align-items: center;
  justify-items: center;
  text-align: center;
