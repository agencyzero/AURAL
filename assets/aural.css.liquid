/*customize button***************************************/
.kissme svg {
    fill: #fa6b10;
	transition: fill 500ms ease-in;
}
.kissme:hover svg {
    fill: #fff;
}
.kissme{
	width:80px;
	position:absolute;
	top: 0px;
	left: 15px;
	opacity:0;
	transition: opacity 500ms ease-out;
	transform: scaleX(-1);
}
.kissme:hover{
	opacity:1;
}

