/*history section*/
.history .byline a
,.history .byline span {
    display: block;
    font-size: .7em;
    text-decoration: none;
    border: 0;
    text-align: right;
    color:/* #b7b7b7*/{{ settings.accent_thr }};
}
.history .page-content{
	padding-bottom:3.2vw;
}
.template-page-history .product-recommendations.with-gutter {
    margin-bottom: 0px;
    margin-top:3vw;
}
.history ul {
    list-style: outside square;
    margin: -1.5em 0 1.5em 2em;
}
.history .paging {
    display: grid;
    grid-template-columns: 1fr 1fr;
}
.history .paging .next{
	text-align:right;
}
.history .paging a {
    font-family: 'Oswald';
    color:{{ settings.sidebar_bg }};
    opacity:.6;
    font-weight: 700;
    font-weight: 900;
    text-decoration: none;
    border: none;
    font-size: 1.5em;
}
.history .paging a:hover{
	/*color:#FA6B0F;*/
	opacity:1;
}
.history .paging a.prev::before {
    content: "\25b6";
    transform: rotate(180deg);
    display: inline-block;
    position: relative;
    top: 3px;
    left: -2px;
}
.history .paging a.next::after {
    content: "\25B6";
    position: relative;
    top: 0;
    left: 2px;
}
.history .image {
    float: right;
    max-width: 50%;
    padding: 2vw;
    margin: 0 0 16px 16px;
    background: #f5f5f5;
}
.history .byline{
    display: grid;
    position: absolute;
    top: 15px;
    right: 20px;
}


/*consignment section*/
.consignment .rte.sidebyside {
