	{{ '//cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css' | stylesheet_tag }}
	{{ '//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css' | stylesheet_tag }}
	{%comment%}{{ 'quickview.scss.css' | asset_url | stylesheet_tag }}{%endcomment%}
	{{ '//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js' | script_tag }}
	{%comment%}{{ 'quickview.js' | asset_url | script_tag }}{%endcomment%}
	<script>
		{%comment%}{% capture content %}{% include 'quickview' %}{% endcapture %}
		var content = {{ content | json }};{%endcomment%}
		
		var isBreakPoint = function (bp) {
			var bps = [320, 480, 768, 948, 1024, 1025, 1200, 1366, 1750],
			w = $(window).width(),
			min, max
			for (var i = 0, l = bps.length; i < l; i++) {
				if (bps[i] === bp) {
					min = bps[i-1] || 0
					max = bps[i]
				break
				}
			}
			return w > min && w <= max
		}

		// main onload handler
		var eargasm = function (){
			$('#content').hide().fadeIn('slow');
			var maxlogo;
			var minlogo;
			if (isBreakPoint(320)){
				$( "body" ).addClass( "aural-320" );
				$( "body" ).addClass( "aural-mobile" );
			} else if (isBreakPoint(480)){
				$( "body" ).addClass( "aural-480" );
				$( "body" ).addClass( "aural-mobile" );
			} else if (isBreakPoint(768)){
				$( "body" ).addClass( "aural-768" );
				$( "body" ).addClass( "aural-mobile" );
			} else if (isBreakPoint(948)){
				$( "body" ).addClass( "aural-948" );
				$( "body" ).addClass( "aural-mobile" );
			} else if (isBreakPoint(1024)){
				$( "body" ).addClass( "aural-1024" );
			} else if (isBreakPoint(1025)){
				minlogo = '50px!important';
				maxlogo = '65px';
				$( "body" ).addClass( "aural-1025" );
			} else if (isBreakPoint(1200)){
				minlogo = '4vw!important';
				maxlogo = '5.2vw';
				$( "body" ).addClass( "aural-1200" );
			} else if (isBreakPoint(1366)){
				minlogo = '4vw!important';
				maxlogo = '5.2vw';
				$( "body" ).addClass( "aural-1366" );
			} else if (isBreakPoint(1750)){
				minlogo = '75px!important';
				maxlogo = '90px';
				$( "body" ).addClass( "aural-1750" );
			}
			$(".logo a").css("max-height", maxlogo);
			setTimeout(
				function() 
				{
					$(".logo a").removeAttr("style");
				}, 3000
			);

			// look for dupe ids and re-sequence?
			$('[id]').each(function(){
  				var ids = $('[id="'+this.id+'"]');
  				if(ids.length>1 && ids[0]==this){
    				console.warn('Multiple IDs #'+this.id);
    				// $(this).prop('id', 'newID');
    				// $(this).attr('id', 'newID');
  				}
			});

			// homepage collection action
			$('.home .collection').on('click', function(){
				var t = $(this).data('target');
		        if(t.indexOf('http')){
		        	document.location = t;
		        }else{
		        	window.open(t);
		        }
		    });
		};
		if(
			document.readyState === "complete" ||
			(document.readyState !== "loading" && !document.documentElement.doScroll)
		){
			eargasm();
		} else {
			document.addEventListener("DOMContentLoaded", eargasm);
		}
		

	</script>

